{
  "version": 2,
  "functions": {
    "app/api/**/*": {
      "maxDuration": 300
    },
    "app/api/templates/route.ts": {
      "maxDuration": 60
    },
    "app/api/invoices/route.ts": {
      "maxDuration": 60
    },
    "app/api/analyze-screen/route.ts": {
      "maxDuration": 120
    },
    "app/api/excel-parse/route.ts": {
      "maxDuration": 300
    },
    "app/api/gemini/route.ts": {
      "maxDuration": 300
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, max-age=0"
        }
      ]
    }
  ],
  "routes": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}